{
  "name": "redux-thunk-action-reducer",
  "version": "0.3.2",
  "description": "Redux Action Reducer with redux thunk",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "prepublish": "npm run build",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neolivz/redux-thunk-action-reducer.git"
  },
  "keywords": [
    "redux",
    "redux-thunk",
    "action",
    "reducer",
    "async"
  ],
  "author": "Jishnu Viswanath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neolivz/redux-thunk-action-reducer/issues"
  },
  "homepage": "https://github.com/neolivz/redux-thunk-action-reducer#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/tests/*.spec.ya(ts|tsx|js)"
    ]
  },
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "jest": "^20.0.4",
    "ts-node": "^3.0.4",
    "tslint-config-standard": "^5.0.2",
    "typescript": "^2.3.3"
  }
